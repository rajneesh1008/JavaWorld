package com.volvo.in.SpringdDemo;

public class Person {

	private int pid;
	private String name;

	public int getPid() {
		
		return pid;
		
	}

	public void setPid(int pid) {
		this.pid = pid;
	}

	public Person() {
		
		
		System.out.println("Object is created");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
